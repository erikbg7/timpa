{
	"name": "svelte-saas-template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"postinstall": "prisma generate --schema=./src/lib/prisma/schema.prisma",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"generate-types": "supabase gen types typescript --project-id xmlhydweyaitfxmkrwag --schema public > types/supabase.ts"
	},
	"prisma": {
		"seed": "vite-node --options.transformMode.ssr='/.*/' src/lib/prisma/seed.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@types/node": "^20.14.2",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"daisyui": "^4.6.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"mdsvex": "^0.11.0",
		"pino-pretty": "^11.2.1",
		"postcss": "^8.4.33",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"prisma": "^5.11.0",
		"supabase": "1.165.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.4.1",
		"ts-node": "^10.9.2",
		"tslib": "^2.4.1",
		"typescript": "^5.3.3",
		"vite": "^5.0.11",
		"vite-node": "^1.6.0"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "5.11.0",
		"@supabase/ssr": "0.1.0",
		"@supabase/supabase-js": "^2.39.2",
		"@tanstack/svelte-query": "^5.35.1",
		"@trpc/client": "^10.45.2",
		"@trpc/server": "^10.45.2",
		"@vercel/analytics": "^1.2.2",
		"assemblyai": "^4.6.1",
		"clsx": "^2.1.0",
		"devalue": "^5.0.0",
		"pino": "^9.2.0",
		"replicate": "^0.30.1",
		"stripe": "^14.19.0",
		"svelte-sonner": "^0.3.24",
		"tailwind-merge": "^2.3.0",
		"tailwind-variants": "^0.2.1",
		"trpc-svelte-query": "^2.1.0",
		"zod": "^3.22.4"
	}
}
